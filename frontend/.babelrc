{
  "presets": ["react", "es2015", "stage-0"],
  "plugins": [
    "transform-runtime",
    "transform-object-rest-spread"
  ],

  "env": {
    "development": {
      "plugins": [
        "typecheck",
        ["react-transform", {
            "transforms": [{
                "transform": "react-transform-catch-errors",
                "imports": ["react", "redbox-react"]
              }
            ]
        }]
      ]
    },
    "production": {
      "presets": ["react-optimize"]
    }
  }
}