{
  "name": "react-redux-boilerplate",
  "version": "0.1.0",
  "description": "React + Redux starter kit for front-end, including simple backend structure.",
  "main": "server.js",
  "scripts": {
    "watch": "webpack --watch --verbose --colors --display-error-details --config webpack/dev.config.js",
    "build-dev": "webpack --verbose --colors --display-error-details --config webpack/dev.config.js",
    "build-prod": "rm -rf ./build/* && webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
    "build-profiling": "webpack --verbose --colors --profiles --display-modules --display-error-details --config webpack/prod.config.js",
    "lint": "eslint -c .eslintrc js/app"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hanselfmu/react-redux-boilerplate.git"
  },
  "author": "Changhao Han",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hanselfmu/react-redux-boilerplate/issues"
  },
  "homepage": "https://github.com/hanselfmu/react-redux-boilerplate#readme",
  "dependencies": {
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-object-rest-spread": "^6.19.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-plugin-typecheck": "^3.9.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "webpack": "^1.13.3"
  }
}
